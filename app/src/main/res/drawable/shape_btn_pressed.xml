<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/blue" />
            <size android:width="@dimen/dp_60" android:height="@dimen/dp_25" />
            <corners android:radius="@dimen/dp_2" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <size android:width="@dimen/dp_60" android:height="@dimen/dp_25" />
            <corners android:radius="@dimen/dp_2" />
            <stroke android:width="@dimen/dp_1" android:color="@color/black" />
        </shape>
    </item>
    <!--<item android:state_pressed="true" android:drawable="@drawable/icon_btn_t"/>-->
    <!--<item android:state_pressed="false" android:drawable="@drawable/icon_btn_f"/>-->
</selector>